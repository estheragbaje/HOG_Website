{
  "kind": "collectionType",
  "collectionName": "sermons",
  "info": {
    "name": "Sermon"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Topic": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "Preacher": {
      "type": "string",
      "required": true
    },
    "Video_url": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Date": {
      "type": "string",
      "required": true
    },
    "Day": {
      "type": "enumeration",
      "enum": [
        "Sunday",
        "Midweek"
      ],
      "required": true,
      "default": "Sunday"
    },
    "Month": {
      "type": "enumeration",
      "enum": [
        "Jan",
        "Feb",
        "Mar",
        "Apr",
        "May",
        "Jun",
        "Jul",
        "Aug",
        "Sept",
        "Oct",
        "Nov",
        "Dec"
      ],
      "required": true
    }
  }
}
